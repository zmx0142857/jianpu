{
  "name": "jianpu",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "description": "Simple music markup language",
  "author": "zmx0142857",
  "license": "MIT",
  "homepage": "https://github.com/zmx0142857/jianpu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmx0142857/jianpu"
  },
  "bugs": "https://github.com/zmx0142857/jianpu/issues",
  "keywords": [
    "jianpu",
    "music markup",
    "music score"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --run",
    "build": "nearleyc src/grammar.ne -o src/grammar.js && node src/after-build.js && tsup",
    "nearley": "nearleyc src/grammar.ne -o src/grammar.js && node src/after-build.js",
    "cov": "vitest --run --coverage"
  },
  "devDependencies": {
    "@types/moo": "^0.5.5",
    "@types/nearley": "^2.11.2",
    "@types/prompts": "^2.4.2",
    "@vitest/coverage-c8": "^0.28.3",
    "bumpp": "^8.2.1",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "tsup": "^6.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.27.2",
    "clsx": "^2.0.0"
  }
}
